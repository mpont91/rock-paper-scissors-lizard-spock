{
  "name": "rock-paper-scissors-lizard-spock",
  "version": "0.0.1",
  "description": "Rock Paper Scissors Lizard Spock game",
  "productName": "Rock Paper Scissors Lizard Spock game",
  "author": "Marc Pont <mpont91@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:ssr": "quasar dev -m ssr",
    "build": "quasar build",
    "build:ssr": "quasar build -m ssr",
    "lint:check": "eslint --ext .js,.vue ./",
    "lint:fix": "eslint --ext .js,.vue --fix ./",
    "format:check": "prettier --check \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "format:fix": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run\"",
    "test:component": "cross-env NODE_ENV=test cypress open-ct",
    "test:component:ci": "cross-env NODE_ENV=test cypress run-ct",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:unit:ui": "vitest --ui"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "pinia": "^2.1.3",
    "quasar": "^2.12.5",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@quasar/app-vite": "^1.4.7",
    "@quasar/quasar-app-extension-testing": "^2.1.1",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^4.2.2",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.1.2",
    "@vitest/ui": "^0.15.0",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.15",
    "eslint": "8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.3",
    "vitest": "^0.15.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
